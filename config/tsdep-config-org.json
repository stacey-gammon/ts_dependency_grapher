{
    "repos": [
        { 
            "fullName": "stacey-gammon/ts_dependency_grapher",
            "clearCache": true,
            "tsconfig": "./tsconfig.json",
            "source": "file",
            "zooms": [0]
        }
    ],
    "excludeFilePaths" : ["_test_", ".test.ts"],
    "outputFolder": "docs/tsdep-org",
    "maxImageSize": "50",
    "buildDocsSite": true,
    "unusedNodeColor": "white",
    "nodeColorWeight": "cluster",
    "takeRecommendations": true,
    "clusteringAlgo": "orgScore",
    "orgScoreMoveThreshold": "10",
    "excludeTypes": true
}