{
    "repos": [
        { 
            "full_name": "elastic/kibana",
            "refresh": false,
            "tsconfig": "../../Elastic/kibana/tsconfig.json",
            "source": "file",
            "entries": [
                { "file": "../../Elastic/kibana/src/plugins/data/public/index.ts", "name": "data" },
                { "file": "../../Elastic/kibana/src/plugins/data_views/public/index.ts", "name": "data_views" },
                { "file": "../../Elastic/kibana/src/plugins/kibana_react/public/index.ts", "name": "kibana_react" },
                { "file": "../../Elastic/kibana/src/plugins/expressions/public/index.ts", "name": "expressions" }
            ],
            "showExternalNodesOnly": false,
            "outputName": "kibana",
            "layoutEngines": [
                { "name": "fdp" },
                { "name": "sfdp" }
            ],
            "extraSourceFileGlobs": [
                "../../Elastic/kibana/x-pack/plugins/**/*{.d.ts,.ts}",
                "../../Elastic/kibana/src/plugins/**/*{.d.ts,.ts}",
                "../../Elastic/kibana/packages/**/*{.d.ts,.ts}"
            ],
            "zooms": [6]
        }
    ],
    "outputFolder": "docs/kibana",
    "excludeFilePaths" : ["_test_", ".test.ts"],
    "maxImageSize": "15",
    "buildDocsSite": true,
    "unusedNodeColor": "white",
    "nodeColorWeight": "OrgScore",
    "clearDotCache": false,
    "refreshParsing": false,
    "takeRecommendations": false
}