{
    "repos": [
        { 
            "full_name": "elastic/kibana",
            "refresh": false,
            "tsconfig": "../../Elastic/kibana/src/plugins/data_views/tsconfig.json",
            "source": "file",
            "showExternalNodesOnly": false,
            "outputName": "data_views",
            "layoutEngines": [
                { "name": "fdp" },
                { "name": "sfdp" }
            ]
        }
    ],
    "excludeFilePaths" : ["_test_", ".test.ts"],
    "outputFolder": "docs/kibana-data-views-org",
    "maxImageSize": "200",
    "buildDocsSite": true,
    "unusedNodeColor": "white",
    "nodeColorWeight": "cluster",
    "takeRecommendations": true,
    "clusteringAlgo": "orgScore",
    "orgScoreMoveThreshold": "8",
    "excludeTypes": true,
    "recommendationFileType": "csv"
}